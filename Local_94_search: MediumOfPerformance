rule "Primo VE Marc - Lsr94"
	when
		MARC is "382"."a" AND
    MARC."362"."2" equals "emnmus"
	then
		create pnx."search"."lsr94" with MARC "382"."a"
end

rule "Primo VE Marc - Lsr94 -2"
	when
		MARC is "382"."b" AND
    MARC."362"."2" equals "emnmus"
	then
		create pnx."search"."lsr94" with MARC "382"."b"
end
