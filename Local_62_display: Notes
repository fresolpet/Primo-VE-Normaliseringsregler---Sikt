rule "Primo VE - Lds62 16"
#518 Målgruppenote (R)
	when
		MARC is "521"."a" AND
		MARC."521".ind"1" equals "#"
	then 
		set TEMP"1" to MARC."521" subfields "a"
		add prefix (TEMP"1","Målgruppe: ")
		create pnx."display"."lds62" with TEMP"1"
end

rule "Primo VE - Lds62 17"
#518 Målgruppenote (R)
	when
		MARC is "521"."a" AND NOT
