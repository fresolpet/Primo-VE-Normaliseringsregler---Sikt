		create pnx."display"."lds62" with TEMP"1"
end


rule "Primo VE - Lds62 18"
#534 Original versjon (R)
	when
		MARC."534" has any "a,b,c,d,e,f,k,l,m,n,o,p,t,x,z" 
	then 
		set TEMP"1" to MARC."534" subfields "p,a,t,b-n"
		set TEMP"2" to MARC."534" subfields "x"
		add prefix (TEMP"2","ISSN: ")
		set TEMP"3" to MARC."534" subfields "z"
		add prefix (TEMP"3","ISBN: ")
		set TEMP"4" to MARC."534" subfields "o"
		add prefix (TEMP"4","Annen identifikator: ")
		concatenate with delimiter (TEMP"1",TEMP"2",". ")
		concatenate with delimiter (TEMP"1",TEMP"3",". ")
		concatenate with delimiter (TEMP"1",TEMP"4",". ")
		create pnx."display"."lds62" with TEMP"1"
end

rule "Primo VE - Lds62 19"
#538 Systemdetaljer og filkarakteristika (R)
	when
		MARC is "538"."a" AND NOT
		MARC."538"."i" match ".*"
	then 
		set TEMP"1" to MARC."538" subfields "a"
		add prefix (TEMP"1","Systemdetaljer: ")
		create pnx."display"."lds62" with TEMP"1"
end

rule "Primo VE - Lds62 20"
#538 Systemdetaljer og filkarakteristika (R)
	when
		MARC is "538"."a" AND
		MARC."538"."i" match ".*"
	then 
		set TEMP"1" to MARC."538" subfields "i,a"
		create pnx."display"."lds62" with TEMP"1"
end

